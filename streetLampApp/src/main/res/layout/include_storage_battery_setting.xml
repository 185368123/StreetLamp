            android:text="@string/lifting_charge_voltage" />

        <EditText
            android:id="@+id/edt_storage_battery_setting_lifting_charge_voltage_value"
            style="@style/rightEditTextStyle2" />
    </LinearLayout>

    <LinearLayout style="@style/LeftRightLinearLayoutStyle" >

        <TextView
            style="@style/LeftTextViewStyle"
            android:text="@string/floating_charge_voltage" />

        <EditText
            android:id="@+id/edt_storage_battery_setting_floating_charge_voltage_value"
