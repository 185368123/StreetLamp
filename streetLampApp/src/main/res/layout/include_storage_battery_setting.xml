<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <TextView
        style="@style/NormalRedTextSizeStyle"
        android:layout_width="match_parent"
        android:layout_height="@dimen/navigation_bar_width"
        android:background="@color/white"
        android:gravity="right|center_vertical"
        android:paddingRight="10dip"
        android:text="@string/already_read" />

    <LinearLayout style="@style/LeftRightLinearLayoutStyle" >

        <TextView
            style="@style/LeftTextViewStyle"
            android:text="@string/storage_battery_capacity" />

        <EditText
            android:id="@+id/edt_storage_battery_setting_storage_battery_capacity_value"
            style="@style/rightEditTextStyle2" />
    </LinearLayout>

    <LinearLayout style="@style/LeftRightLinearLayoutStyle" >

        <TextView
            style="@style/LeftTextViewStyle"
            android:text="@string/storage_battery_type" />

        <EditText
            android:id="@+id/edt_storage_battery_setting_storage_battery_type_value"
            style="@style/rightEditTextStyle2" />
    </LinearLayout>

    <LinearLayout style="@style/LeftRightLinearLayoutStyle" >

        <TextView
            style="@style/LeftTextViewStyle"
            android:text="@string/overpressure_voltage" />

        <EditText
            android:id="@+id/edt_storage_battery_setting_overpressure_voltage_value"
            style="@style/rightEditTextStyle2" />
    </LinearLayout>

    <LinearLayout style="@style/LeftRightLinearLayoutStyle" >

        <TextView
            style="@style/LeftTextViewStyle"
            android:text="@string/charging_limit_voltage" />

        <EditText
            android:id="@+id/edt_storage_battery_setting_charging_limit_voltage_value"
            style="@style/rightEditTextStyle2" />
